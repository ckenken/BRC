package com.slidingmenu.example;

import android.os.Bundle;
import android.view.Menu;
import android.view.ViewGroup.LayoutParams;
import android.widget.*;

import com.slidingmenu.example.R;


public class HomeActivity extends BaseActivity {
	
	private TableLayout table_initiate;
	private TableRow table_row;
	private TextView view;
	
	public HomeActivity() {
		super(R.string.title_bar_content);
	}
	
	@Override
	public void onCreate(Bundle savedInstanceState) {
		super.onCreate(savedInstanceState);

		// set the Above View
		setContentView(R.layout.home);
		setSlidingActionBarEnabled(true);
		
		//load
		
		table_initiate = (TableLayout) findViewById(R.id.table_initiate);
		
		table_row = new TableRow(this);
		
		
		view = new TextView(this);
		
		view.setText("主購者代號");		
		table_row.addView(view);
		
		view.setText("商品名稱");		
		table_row.addView(view);
		
		view.setText("取貨地區");		
		table_row.addView(view);
		
		view.setText("截止日期");		
		table_row.addView(view);
		
		view.setText("進入");		
		table_row.addView(view);
		
		table_initiate.addView(table_row);
		
		
		
		
		
		
	}


}
