		 "台北市", "新北市", "台南市", "高雄市", "苗粟縣",
		 "台北市", "新北市", "台南市", "高雄市", "苗粟縣",
		 "台北市", "新北市", "台南市", "高雄市", "苗粟縣",
		 "台北市", "新北市", "789", "cde", "abc"
		 };
	
		 
		 private static final String[] mFoods = new String[] {
		 "大餅包小餅", "蚵仔煎", "東山鴨頭", "臭豆腐", "潤餅",
		 "豆花", "青蛙下蛋","豬血糕", "大腸包小腸", "鹹水雞",
		 "烤香腸","車輪餅","珍珠奶茶","鹹酥雞","大熱狗",
		 "炸雞排","山豬肉","花生冰","剉冰","水果冰",
